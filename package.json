{
  "name": "react-chrono",
  "version": "3.0.0",
  "license": "MIT",
  "packageManager": "pnpm@10.15.1",
  "description": "A Modern Timeline component for React",
  "keywords": [
    "timeline",
    "react-timeline",
    "horizontal-timeline",
    "vertical-timeline"
  ],
  "author": "Prabhu Murthy",
  "repository": {
    "url": "https://github.com/prabhuignoto/react-chrono",
    "type": "git"
  },
  "scripts": {
    "ladle": "ladle serve",
    "ladle:build": "ladle build",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:ci": "playwright test --reporter=github --reporter=html",
    "test:e2e:shard": "playwright test --shard=$SHARD",
    "test:ct": "playwright test -c playwright-ct.config.ts",
    "test:ct:ui": "playwright test -c playwright-ct.config.ts --ui",
    "test:integration": "playwright test -c playwright.integration.config.ts",
    "test:integration:ui": "playwright test -c playwright.integration.config.ts --ui",
    "test:integration:headed": "playwright test -c playwright.integration.config.ts --headed",
    "test:report": "playwright show-report",
    "test:trace": "playwright show-trace",
    "test:codegen": "playwright codegen",
    "test:install": "playwright install --with-deps",
    "test:update-snapshots": "playwright test --update-snapshots",
    "eslint": "eslint src/**/*.{tsx,ts} --no-warn-ignored",
    "fix-js": "eslint src/**/*.{tsx,ts} --fix --no-warn-ignored",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:all": "pnpm eslint && pnpm lint",
    "find-bugs": "npx tsc --noEmit && pnpm lint:all && pnpm test",
    "find-bugs:full": "./scripts/find-bugs.sh",
    "snyk": "snyk test",
    "dev": "vite --config vite.site.config.mts --host",
    "clean": "npx concurrently \"pnpm format\" \"pnpm lint:all\"",
    "test": "vitest --config vitest.config.mts",
    "test-ui": "vitest --config vitest.config.mts --ui",
    "vite:build": "vite build --config vite.config.mts",
    "build:site": "vite build --config vite.site.config.mts",
    "preview:site": "vite preview --config vite.site.config.mts",
    "coveralls": "pnpm exec coveralls < coverage/lcov.info",
    "size-limit": "pnpm vite:build && pnpm size-limit",
    "build": "vite build --config vite.config.mts",
    "build:analyze": "ANALYZE=true vite build --config vite.config.mts",
    "fix-css": "stylelint src/**/*style.ts --fix"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "dayjs": "^1.11.18",
    "use-debounce": "^10.0.6",
    "xss": "^1.0.15"
  },
  "peerDependencies": {
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0"
  },
  "publishConfig": {
    "cache": "cache/.npm",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-optional-chaining": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime": "^7.28.4",
    "@eslint/compat": "^1.4.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@ladle/react": "^5.1.0",
    "@playwright/experimental-ct-react": "^1.56.0",
    "@playwright/test": "^1.56.0",
    "@size-limit/preset-big-lib": "^11.2.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/dynamic": "^2.1.5",
    "@vanilla-extract/recipes": "^0.5.7",
    "@vanilla-extract/sprinkles": "^1.6.5",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "babel-plugin-jsx-remove-data-test-id": "^3.0.0",
    "cssnano": "^7.1.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^3.3.0",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^10.4.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^3.6.2",
    "react-router-dom": "^7.9.4",
    "rollup-plugin-visualizer": "^6.0.4",
    "size-limit": "^11.2.0",
    "snyk": "^1.1300.0",
    "stylelint": "^16.25.0",
    "stylelint-config-recess-order": "^7.3.0",
    "stylelint-config-recommended": "^17.0.0",
    "stylelint-order": "^7.0.0",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vite-plugin-dts": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "resolutions": {
    "ansi-regex": "5.0.1",
    "json-schema": "0.4.0"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css",
    "*.css.ts"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.esm.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "size-limit": [
    {
      "path": "dist/index.cjs",
      "limit": "250 KB"
    },
    {
      "path": "dist/index.esm.js",
      "limit": "250 KB"
    }
  ],
  "browserslist": [
    "last 3 versions",
    "not ie <= 11",
    "not dead",
    "> 5%"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "engines": {
    "node": ">=22"
  }
}
