{
  "name": "react-chrono",
  "version": "3.3.2",
  "license": "MIT",
  "packageManager": "bun@latest",
  "description": "A Modern Timeline component for React",
  "keywords": [
    "timeline",
    "react-timeline",
    "horizontal-timeline",
    "vertical-timeline"
  ],
  "author": "Prabhu Murthy",
  "repository": {
    "url": "https://github.com/prabhuignoto/react-chrono",
    "type": "git"
  },
  "scripts": {
    "ladle": "ladle serve",
    "ladle:build": "ladle build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "dotenv -e .env.local -- chromatic --exit-zero-on-changes",
    "test:e2e": "playwright test -c config/playwright/playwright.config.ts",
    "test:e2e:ui": "playwright test -c config/playwright/playwright.config.ts --ui",
    "test:e2e:debug": "playwright test -c config/playwright/playwright.config.ts --debug",
    "test:e2e:headed": "playwright test -c config/playwright/playwright.config.ts --headed",
    "test:e2e:chrome": "playwright test -c config/playwright/playwright.config.ts --project=chromium",
    "test:e2e:firefox": "playwright test -c config/playwright/playwright.config.ts --project=firefox",
    "test:e2e:webkit": "playwright test -c config/playwright/playwright.config.ts --project=webkit",
    "test:e2e:mobile": "playwright test -c config/playwright/playwright.config.ts --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:ci": "playwright test -c config/playwright/playwright.config.ts --reporter=github --reporter=html",
    "test:e2e:shard": "playwright test -c config/playwright/playwright.config.ts --shard=$SHARD",
    "test:ct": "playwright test -c config/playwright/playwright-ct.config.ts",
    "test:ct:ui": "playwright test -c config/playwright/playwright-ct.config.ts --ui",
    "test:integration": "playwright test -c config/playwright/playwright.integration.config.ts",
    "test:integration:ui": "playwright test -c config/playwright/playwright.integration.config.ts --ui",
    "test:integration:headed": "playwright test -c config/playwright/playwright.integration.config.ts --headed",
    "test:report": "playwright show-report",
    "test:trace": "playwright show-trace",
    "test:codegen": "playwright codegen",
    "test:install": "playwright install --with-deps",
    "test:update-snapshots": "playwright test --update-snapshots",
    "eslint": "eslint --config config/build/eslint.config.mjs src/**/*.{tsx,ts} --no-warn-ignored",
    "fix-js": "eslint --config config/build/eslint.config.mjs src/**/*.{tsx,ts} --fix --no-warn-ignored",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:all": "bun eslint && bun lint",
    "find-bugs": "npx tsc --noEmit && bun lint:all && bun test",
    "find-bugs:full": "./scripts/find-bugs.sh",
    "snyk": "snyk test",
    "dev": "vite --config config/build/vite.site.config.mts --host",
    "clean": "npx concurrently \"bun format\" \"bun lint:all\"",
    "test": "vitest --config config/build/vitest.config.mts --update",
    "test-ui": "vitest --config config/build/vitest.config.mts --ui",
    "test:perf": "vitest --config config/build/vitest.config.mts benchmark-tests/performance.test.tsx",
    "test:perf:setup": "./benchmark-tests/setup.sh",
    "benchmark": "node benchmark-tests/benchmark-suite.js",
    "vite:build": "vite build --config config/build/vite.config.mts",
    "build:site": "vite build --config config/build/vite.site.config.mts",
    "preview:site": "vite preview --config config/build/vite.site.config.mts",
    "coveralls": "bunx coveralls < coverage/lcov.info",
    "size-limit": "bun vite:build && bun size-limit",
    "build": "vite build --config config/build/vite.config.mts",
    "build:analyze": "ANALYZE=true vite build --config config/build/vite.config.mts",
    "lint:css": "stylelint --config config/style/stylelint.config.js config/style/_placeholder.css",
    "fix-css": "stylelint --config config/style/stylelint.config.js --fix config/style/_placeholder.css",
    "knip": "knip"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "dayjs": "^1.11.19",
    "use-debounce": "^10.0.6",
    "xss": "^1.0.15"
  },
  "peerDependencies": {
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  },
  "publishConfig": {
    "cache": "cache/.npm",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@ladle/react": "^5.1.1",
    "@playwright/experimental-ct-react": "^1.57.0",
    "@playwright/test": "^1.57.0",
    "@size-limit/preset-big-lib": "^11.2.0",
    "@storybook/addon-docs": "10.0.8",
    "@storybook/addon-themes": "10.0.8",
    "@storybook/react-vite": "10.0.8",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vanilla-extract/css": "^1.17.5",
    "@vanilla-extract/dynamic": "^2.1.5",
    "@vanilla-extract/recipes": "^0.5.7",
    "@vanilla-extract/sprinkles": "^1.6.5",
    "@vanilla-extract/vite-plugin": "^5.1.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "autoprefixer": "^10.4.22",
    "babel-plugin-jsx-remove-data-test-id": "^3.0.0",
    "chromatic": "^13.3.4",
    "cssnano": "^7.1.2",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-storybook": "10.0.8",
    "eslint-plugin-typescript-sort-keys": "^3.3.0",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "knip": "^5.72.0",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^10.5.0",
    "prettier": "^3.7.4",
    "react-router-dom": "^7.10.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "size-limit": "^11.2.0",
    "snyk": "^1.1301.0",
    "storybook": "10.0.8",
    "stylelint": "^16.26.1",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-recommended": "^17.0.0",
    "stylelint-order": "^7.0.0",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15"
  },
  "resolutions": {
    "ansi-regex": "5.0.1",
    "json-schema": "0.4.0"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css",
    "*.css.ts"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.esm.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "size-limit": [
    {
      "path": "dist/index.cjs",
      "limit": "250 KB"
    },
    {
      "path": "dist/index.esm.js",
      "limit": "250 KB"
    }
  ],
  "browserslist": [
    "last 3 versions",
    "not ie <= 11",
    "not dead",
    "> 5%"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "engines": {
    "node": ">=22"
  }
}
