{
  "name": "react-chrono",
  "version": "1.9.2",
  "license": "MIT",
  "description": "A Modern Timeline component for React",
  "keywords": [
    "timeline",
    "react-timeline",
    "horizontal-timeline",
    "vertical-timeline"
  ],
  "author": "Prabhu Murthy",
  "repository": {
    "url": "https://github.com/prabhuignoto/react-chrono",
    "type": "git"
  },
  "scripts": {
    "build": "snowpack build",
    "eslint": "eslint ./src/**/*.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:css": "stylelint ./src/**/*style.ts",
    "rollup": "rimraf ./dist && rollup -c",
    "start": "snowpack dev --polyfill-node",
    "lint:all": "yarn run eslint && yarn run lint:css && yarn run lint",
    "cypress": "cypress open",
    "cypress:test": "start-server-and-test start http://localhost:8080 cypress",
    "cypress:record": "cypress run --record --key 0a753295-339a-4351-bf1d-6b91d4bcb937",
    "cypress:runner": "start-server-and-test start http://localhost:8080 cypress:record",
    "test": "jest -u"
  },
  "dependencies": {
    "@emotion/core": "^11.0.0-next.10",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "classnames": "^2.2.6",
    "focus-visible": "^5.2.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-optional-chaining": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@emotion/babel-plugin": "^11.2.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-strip": "^2.0.0",
    "@snowpack/app-scripts-react": "^2.0.1",
    "@snowpack/plugin-dotenv": "^2.0.5",
    "@snowpack/plugin-react-refresh": "^2.4.1",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@snowpack/web-test-runner-plugin": "^0.2.2",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.3",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/snowpack-env": "^2.3.3",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-loader": "^8.2.2",
    "cssnano": "^4.1.10",
    "cypress": "^6.8.0",
    "eslint": "^7.22.0",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.8",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.42.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "snowpack": "^3.1.2",
    "start-server-and-test": "^1.12.1",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended": "^4.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.11.0",
    "**/@typescript-eslint/parser": "^4.11.0"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "src/components/**/*.{ts,tsx}": [
      "prettier --write",
      "stylelint ./src/**/*style.ts",
      "eslint ./src/**/*.tsx",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    "last 3 versions",
    "not ie <= 11",
    "not dead",
    "> 5%"
  ],
  "main": "dist/react-chrono.js",
  "module": "dist/react-chrono.esm.js",
  "umd": "dist/react-chrono.umd.js"
}
